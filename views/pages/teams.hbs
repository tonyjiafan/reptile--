{{#each listObj}}
<div style="width:600px;border-radius:5px;padding:5px 10px;margin:10px auto;background:#e8e8e8;text-align:center;">
  <img style="display:block;margin:10px auto 20px;width;100px;height:100px;" src={{teamIcon}} alt="球队logo" />
  <p>
    <span style="color:orange;padding-right20px;font-size:20px;font-weight:700;">球队: {{teamName}}</span>
    <span style="color:#fc5144;padding-right20px;font-size:20px;font-weight:700;">目前战绩: {{teamRecord}}</span>
  </p>
</div>
{{/each}}
